<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:priv-android="http://schemas.android.com/apk/prv/res/android"
    android:autoMirrored="true">
    <item android:height="10dp">
        <shape
            android:tint="?android:attr/colorAccent"
            android:shape="rectangle">
                <size android:height="10dp" />
                <stroke
                    android:width="1dp"
                    android:color="@android:color/seekbar_progress" />
                <solid android:color="@android:color/seekbar_progress" />
                <corners android:radius="5dp" />
        </shape>
    </item>
    <item android:gravity="end|center" android:end="4dp" android:height="12dp">
        <shape android:tint="?android:attr/colorBackground" android:shape="oval">
            <size android:width="12dp" android:height="12dp" />
            <solid android:color="#80626161" />
        </shape>
    </item>
</layer-list>
